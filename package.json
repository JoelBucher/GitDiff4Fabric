{
  "name": "gitdiff4fabric",
  "displayName": "GitDiff4Fabric",
  "description": "Minimal VS Code extension that lets you diff Fabric workspaces.",
  "version": "0.0.2",
  "publisher": "JoelBucher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoelBucher/GitDiff4Fabric"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:git-diff-4-fabric.login"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-diff-4-fabric.compare",
        "title": "GitDiff4Fabric: Compare Directory to Workspace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  }
}
