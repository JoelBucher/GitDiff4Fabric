{
  "name": "gitdiff4fabric",
  "displayName": "GitDiff4Fabric",
  "description": "Minimal VS Code extension that lets you diff workspaces.",
  "version": "0.0.1",
  "publisher": "joel-bucher",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fabric-auth-demo.login"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fabric-auth-demo.login",
        "title": "GitDiff4Fabric: Login"
      },
      {
        "command": "fabric-auth-demo.downloadNotebooks",
        "title": "GitDiff4Fabric: Download Notebooks"
      },
      {
      "command": "fabric-auth-demo.gitStatus",
      "title": "GitDiff4Fabric: Check Git Status"
    }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  }
}
